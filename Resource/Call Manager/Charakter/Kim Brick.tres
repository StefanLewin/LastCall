[sub_resource type="Resource" id="Resource_lyjul"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://b1n2bqj437ct6"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Danke, dass Sie sich die Zeit nehmen. Können Sie mir etwas mehr erzählen über die Tage vor Noras… Tod?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_4p4ur"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://b1n2bqj437ct6"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Sie war… anders. Still, sehr still. Und ständig am Handy. Sie wirkte oft abwesend, als würde sie… auf etwas warten. Ich habe sie gefragt, was los sei - aber sie hat mir keine Antwort gegeben."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_oavjr"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://ktikamxaxs5m"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Hat sie Ihnen denn gar nichts anvertraut? Oder gab es vielleicht andere Personen in ihrem Umfeld, mit denen sie darüber gesprochen haben könnte?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_45b17"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://c0m8j6jvhim8i"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Sie… hat manchmal gesagt, dass sie sich beobachtet fühlt. Wortwörtlich: „Die Augen hören nicht auf, mich anzusehen.“ Ich habe sie ausgelacht - naja, vielleicht nicht ausgelacht, aber ich habe gesagt, dass sie sich das nur einbildet. Sie… war da schon sehr empfindlich."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_7shvl"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://cpj03kvqsxu78"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Ihre Mutter erwähnte, dass Nora Angst hatte - nicht nur um sich, sondern auch um Sie. Erkennen Sie sich darin wieder?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_fmtwy"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://bhsro4ryqnyxg"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Nein… also… vielleicht. Sie sagte einmal: „Wenn mir nichts passiert, dann vielleicht dir.“ - Ich habe das nicht ernst genommen. Ich meine, wer sollte mir schon etwas tun? Aber… Nora konnte in den letzten Wochen ziemlich… dramatisch sein."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_k0d2j"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://bhkgw574y0fud"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Dramatisch?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_wldyw"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://dhdpbp4t6hxv8"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Sie hat über einen Brief gesprochen, der auf ihrem Schreibtisch lag, „der nicht von ihr sei“. Ich habe sie gefragt, was das bedeutet, aber sie wollte es mir nicht zeigen. Vielleicht war da gar nichts. Vielleicht wollte sie nur Aufmerksamkeit."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_i6ou0"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://cxogdslbd33tx"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Sind Sie sicher, dass es nur das war?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_ftn3q"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://b3j3ihtb341dv"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Ich weiß nicht, Detective. Vielleicht hat sie auch einfach… Dinge gesehen, die nicht da waren. Ich wollte helfen. Wirklich. Aber sie hat mir nicht vertraut."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_qsnxd"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://cxogdslbd33tx"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Gab es sonst jemanden, mit dem Nora in letzter Zeit Schwierigkeiten hatte?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_86n6t"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://ckbmwe5fthvpx"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Ihren Exfreund. [url=105]Brian Hopkins[/url]. Sie hat seinen Namen ein paar Mal erwähnt, aber nie direkt gesagt, was los war. Nur, dass sie Angst hatte, er würde nicht loslassen. Ob sie ihn noch getroffen hat, weiß ich nicht. Aber… wenn Sie Antworten suchen, sollten Sie mit ihm reden."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_qehn2"]
