[sub_resource type="Resource" id="Resource_k2qk1"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://b4ry86ejtoiwn"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Danke, dass Sie erneut Zeit für uns haben, Kim. Ich möchte noch einmal über die Nacht von Noras Tod sprechen. Können Sie mir sagen, wo Sie zur Tatzeit gewesen sind?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_6shmu"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://bridekel2rp01"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Ich war zu Hause. Ich habe den ganzen Abend dort verbracht, Serien geschaut, Nachrichten gecheckt… allein. Niemand kann mir das widerlegen, aber mein Handy zeigt es. Ich habe keine Ahnung, was mit Nora passiert ist, als sie unter der Brücke war."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_g65g2"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://c4rk5kt11pn8q"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Gab es irgendeine Begegnung oder etwas Auffälliges an diesem Tag?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_0jxdt"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://clg4yn1kxru1r"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Nein, ehrlich nicht. Ich habe mich nicht mit ihr getroffen. Ich habe sie nur ein paar Stunden vorher kurz angerufen, aber sie hat nicht abgenommen."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_d7jvw"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://bxe1382u1u5ny"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Haben Sie jemals bemerkt, dass [url=106]Neil Hopkins[/url] besonders auf Noras Verhalten reagiert hat?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_og45o"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://cki3t4f30gkkw"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Ja. Er ist extrem auf die [url=301]„Familienehre“[/url] bedacht, wie er es nennt. Ich meine… wirklich extrem. Er hat immer betont, wie wichtig es ist, dass alles in der [url=301]Familie „sauber“[/url] aussieht. Ich habe nie verstanden, warum [url=106]er[/url] sich so auf Nora fixiert hat."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_hl4a0"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://uq1m2moh2pb1"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Hat er Ihnen gegenüber jemals Andeutungen gemacht, wie er wollte, dass Nora sich verhält?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_oig4w"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://6x4m3k7lkm5n"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Ja, er hat öfter kommentiert, dass sie „sich zusammenreißen“ soll oder „die Dinge richtig stellen“ müsse, weil sonst alles schlecht für die Familie aussieht. Ich dachte damals, er übertreibt… aber vielleicht war es mehr, als nur Kontrolle."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_kf0sm"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://dim66rjg23pme"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Gab es sonst jemanden, der Druck auf Nora ausgeübt hat?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_bx2kq"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://c8wgfa2htp63a"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Kim Bricks"
Text = "Brian, ja… er war ihr [url=105]Ex[/url]. Aber [url=106]Neil…[/url] er war immer da, hat jede ihrer Entscheidungen irgendwie bewertet. Ich habe sie nie so gesehen, als hätte sie sich freiwillig bedroht gefühlt - außer vielleicht durch seinen ständigen Druck."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_y4hbw"]
script = ExtResource("1_4p4ur")
AudioPath = "uid://df2h28fyk1a63"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Danke, Kim. Ihre Angaben helfen uns, die Umstände besser zu verstehen."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

