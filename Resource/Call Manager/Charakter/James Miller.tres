[sub_resource type="Resource" id="Resource_yspfh"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://clm6riuxnop76"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Anita Zelmer"
Text = "Ich starte jetzt die Aufnahme für den Bericht. (Piep) Hier ist Dr. Anita Zelmler, die leitende Pathologin der städtischen Autopsie. Die Autopsie wird bei Nora Hickens durchgeführt. Sie ist 23 Jahre alt und wurde um 10 Uhr unter der [url=401]Waldbrücke im Flussbett im Bay-Distrikt[/url] gefunden. Nach grober äußerlicher Sichtung kann man erkennen, dass die Obduzierte durch die noch anhaltende Totenstarre noch innerhalb der letzten 24 bis 48 Stunden gestorben sein müsste. Durch die Totenflecken kann man das ganze auf 8 bis 10 Stunden eingrenzen. An Fingerkuppen und Ohrläppchen erkennt man bereits Verwesung."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_7iqah"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://bw7jco0oiw426"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "James Miller"
Text = "Das hört sich sehr unangenehm an."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_7fh1q"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://r7jk8w6s12t7"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Anita Zelmer"
Text = "Am ganzen Körper sind starke Prellungen zu erkennen, die wahrscheinlich von einem Sturz kommen könnten. Am Kopf ist eine starke Kopfverletzung zu sehen, wo sich schon Käferlarven eingenistet haben. Es sieht nach einem... Ja. es ist ein Schädelbruch. Hohe Wahrscheinlichkeit, dass sie an diesem Blutverlust gestorben ist. Aufnahme beenden. (Piep) Hat es dir weitergeholfen?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_6f1pg"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://bngd61c7fxeug"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "James Miller"
Text = "Es hat mir sehr weitergeholfen. Also für mich wirkt es wie ein [url=203]Selbstmord[/url], wenn man den Abschiedsbrief beachtet."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

