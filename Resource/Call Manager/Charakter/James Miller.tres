[sub_resource type="Resource" id="Resource_hjlpj"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://br0pmvhvr23gp"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "James Miller"
Text = "Detective Clarkson, hier spricht Miller. Ich melde mich direkt vom Tatort unterhalb der [url=401]Brücke im Wald[/url]. Der Bereich ist mittlerweile abgesperrt, die Spurensicherung arbeitet, und der Suchtrupp durchkämmt gerade die Umgebung. Die Rechtsmedizin hat die Leiche bereits abtransportiert - ich werde später mit Frau Zelmer Rücksprache halten."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_wtbem"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://br0pmvhvr23gp"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Verstanden. Ich werde ebenfalls mit ihr sprechen, sobald die ersten Ergebnisse vorliegen. Hat der Suchtrupp schon etwas Sicheres gefunden?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_p718g"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://cxlaqirq8toq1"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "James Miller"
Text = "Ja, ein [url=201]Messer[/url] - etwa hundert Meter flussabwärts, fast schon an der Mündung zum Meer. Aber ehrlich gesagt: die Chancen auf verwertbare Fingerabdrücke stehen schlecht. Das Wasser dürfte alles abgewaschen haben."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_gl2pt"]
script = ExtResource("1_8y6ul")
AudioPath = "uid://st0e458on3ff"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Notieren Sie es trotzdem als Beweisstück und lassen Sie es ins Labor geben. Wir dürfen nichts ausschließen. Ich komme gleich runter zum Tatort, dann sprechen wir alles Weitere durch."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

