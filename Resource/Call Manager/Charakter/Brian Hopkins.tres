IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Danke, dass Sie sich die Zeit nehmen, Brian. Können Sie mir sagen, wann Sie das letzte Mal Kontakt zu Nora hatten?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_cndui"]
script = ExtResource("1_oogro")
AudioPath = "uid://c1fu54c7m4cxc"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Brian Hopkins"
Text = "Es war ein paar Wochen her. Wir haben telefoniert, das war alles. [url=302]Ich habe Abstand gehalten[/url], weil… nun ja, weil sie sich verändert hat."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_oogro"]
script = ExtResource("1_oogro")
AudioPath = "uid://ce8lkgsmftru5"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Inwiefern?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_d7uwe"]
script = ExtResource("1_oogro")
AudioPath = "uid://celbdl38mwgq6"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Brian Hopkins"
Text = "Sie wurde immer stiller, zurückgezogener. Manchmal wirkte sie… überängstlich. Sie hat ständig über Dinge gesprochen, die ich nie wirklich nachvollziehen konnte. Über Briefe, Nachrichten… sie meinte immer, jemand würde sie beobachten. Ich habe versucht, sie zu beruhigen, aber sie hat alles überdramatisiert."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_1mm5d"]
script = ExtResource("1_oogro")
AudioPath = "uid://cuq0nm6vtosk7"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Gab es zwischen Ihnen Streit?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_xoqio"]
script = ExtResource("1_oogro")
AudioPath = "uid://bmrrxk3uqo1ri"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Brian Hopkins"
Text = "Natürlich, wir hatten unsere Probleme. Aber ich habe nie wirklich etwas falsch gemacht. Nora war… kompliziert, ich weiß nicht, wie ich es anders sagen soll. Sie hat oft Dinge angestellt, die ich nicht verstand."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_wgbi4"]
script = ExtResource("1_oogro")
AudioPath = "uid://b46a5uaqy27vl"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Gab es sonst jemanden, der Einfluss auf sie hatte?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_w7wjv"]
script = ExtResource("1_oogro")
AudioPath = "uid://xlnt4qtkgb0m"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Brian Hopkins"
Text = "Neil… mein [url=106]Bruder[/url]. Sie hat ihn ab und zu erwähnt. Ich weiß nicht genau, warum sie so reagierte, aber sie wirkte nervös, wenn sein Name fiel. Vielleicht hat sie sich Dinge eingebildet - das kann ich nicht sagen."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_uoq6p"]
script = ExtResource("1_oogro")
AudioPath = "uid://cfidsbgt1wof1"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Also glauben Sie, dass jemand Druck auf sie ausgeübt haben könnte?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_i3atx"]
script = ExtResource("1_oogro")
AudioPath = "uid://cfidsbgt1wof1"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Brian Hopkins"
Text = "Ich weiß nicht. Sie hat selbst oft alles dramatisiert. Ich meine… wer weiß schon, was in ihrem Kopf vorging. Wenn Sie Antworten suchen, sollten Sie vielleicht trotzdem mit [url=106]Neil Hopkins[/url] sprechen."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

