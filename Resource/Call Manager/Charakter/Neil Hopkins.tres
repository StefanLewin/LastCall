AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Unknown"
Text = "Gab es Situationen, in denen Nora nervös oder verängstigt wirkte?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_4drim"]
script = ExtResource("1_siens")
AudioPath = "uid://52p8ribqwe1u"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Neil Hopkins"
Text = "Sie war eigenwillig, ja. Sie machte sich Sorgen über Dinge, die ich nicht nachvollziehen konnte - Briefe, Nachrichten, so etwas. Ich habe sie nicht bedroht. Sie war manchmal sehr emotional."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

