Text = "Gab es jemanden, von dem Sie wissen, dass er Druck auf sie ausgeübt haben könnte?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_bdugr"]
