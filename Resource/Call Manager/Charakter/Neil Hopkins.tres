[sub_resource type="Resource" id="Resource_gywrx"]
script = ExtResource("1_siens")
AudioPath = "uid://iyruiqtoiank"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Neil Hopkins"
Text = "Ich habe sie gelegentlich gesehen. Nicht oft. Nach der Trennung zog sie sich zurück. Ich wollte nicht aufdringlich sein. Sie wissen ja… für mich ist es immer wichtig, dass [url=301]die Familie in Ordnung dasteht[/url], dass keine Dinge öffentlich aus dem Ruder laufen."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_wfnxk"]
