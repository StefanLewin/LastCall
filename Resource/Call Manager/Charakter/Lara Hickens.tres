[sub_resource type="Resource" id="Resource_mgod0"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://cdnhovcoicnu4"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Miss Hittgens, können Sie mir sagen, ob Ihnen in den letzten Tagen bei Ihrer Tochter etwas Besonderes aufgefallen ist?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_wxgwm"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://cqcmbcxl2de2e"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Lara Hickens"
Text = "Nora hat sich sehr stark zurückgezogen. Sie verbrachte fast jede freie Minute in ihrem Zimmer. Manchmal hörte ich, wie sie sprach - doch wenn ich die Tür öffnete, zog sie hastig ihr Handy weg oder tat so, als sei nichts gewesen."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_nftf3"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://b23kbydfvy0j6"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Haben Sie sie gefragt, was los sei?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_5l6mi"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://divyb6w507dog"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Lara Hickens"
Text = "Natürlich. Aber sie wich mir immer aus, sie sagte: \\\"Alles in Ordnung, Mama.\\\" So kannte ich meine Tochter nicht."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_u1gqe"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://r4h8ssgquqmj"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Gab es jemanden, mit dem sie vertrauter sprach?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_i65pl"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://c56ejwfmc0lh0"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Lara Hickens"
Text = "Ihre beste Freundin, [url=103]Kim Bricks[/url]. Kim durfte noch in ihr Zimmer. Wenn Nora lachte, dann mit ihr - aber auch dieses Lachen klang gezwungen, fremd. Einmal hörte ich, wie sie zu Kim sagte: \\\"Ich habe das Gefühl, dass mich die Augen immer beobachten.\\\" Ich weiß bis heute nicht, was sie damit meinte. Wurde sie verfolgt? Bedroht? Aber… warum hat sie uns nichts davon erzählt?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_6esc7"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://bobt8dyy0rxc3"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Detective Clark"
Text = "Können Sie sich noch an den genauen Wortlaut erinnern?"
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_b3nxg"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://dcqwg2i3l10it"
AudioDelay = 0.0
IsPlayerLine = false
SpeakerName = "Lara Hickens"
Text = "Nicht vollständig. Aber es klang, als hätte sie nicht nur Angst um sich, sondern auch um Kim. Als ob jemand ihnen beiden etwas antun könnte."
Id = 0
metadata/_custom_type_script = "uid://bmxfs2x784mm7"

[sub_resource type="Resource" id="Resource_6332w"]
script = ExtResource("1_u1gqe")
AudioPath = "uid://b188i0gso0q1i"
AudioDelay = 0.0
